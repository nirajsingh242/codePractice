package designPatternStructural;

interface Component
{
	void showPrice();
}

class Leaf implements Component
{
	int price;
	String name;

	public Leaf(int price, String name) {
		super();
		this.price = price;
		this.name = name;
	}

	@Override
	public void showPrice() {
		
		
	}
	
}

class Composite implements Component
{
	public Composite(String name) {
		super();
		this.name = name;
	}
	String name;
	@Override
	public void showPrice() {
		
		
	}
	
}