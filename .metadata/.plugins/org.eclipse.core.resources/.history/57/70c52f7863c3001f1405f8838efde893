package threadseExample;

public class ThreadYieldExample implements Runnable{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=0;i<5;i++)
		{
			System.out.println("run method thread "+i);
		}

	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		for(int i=0;i<5;i++)
		{
			System.out.println("run method thread "+i);
		}

		
	}

}
